       function daysBackToMon() {
           var NewDate = new Date();
           i = 7;
           daysback = 0;
           while (i >= 0) {
               i--;
               NewDate.setDate(NewDate.getDate() - i);
               var daynum = NewDate.getUTCDay();
             
               if (parseInt(daynum) == 1) {
                   daysback = i;
               }
               daysback++;
           }

           return daysback;

       }

